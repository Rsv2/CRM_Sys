<Window x:Class="Desktop.ImagesWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Desktop"
        mc:Ignorable="d" Background="{DynamicResource winColor}"
        Title="Изображения" Height="630" Width="880">
    <Grid>
        <Viewbox HorizontalAlignment="Left" VerticalAlignment="Top">
            <local:PicturesUI x:Name="PicContext" Height="600" Width="850" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        </Viewbox>
    </Grid>
</Window>
